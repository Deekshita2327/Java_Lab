1.package Com.sample;
class RBIbank{

	public double rate() {
		return 2;
	}
}
class SBI extends RBIbank{
	
	public double rate() {
		return 3;
	}
	public void loan() {
		System.out.println("SBI gives loan up to 1lakh");
	}
}
class ICICI extends RBIbank{
	public double rate() {
		return 4;
	}
}

public class Inheritance {
	public static void main(String[] args) {
		RBIbank b = new RBIbank();
		SBI sb = new SBI();
		ICICI ic = new ICICI();
		RBIbank rb = new SBI();
		System.out.println("the rate of interest in RBI is" + b.rate());
		System.out.println("the rate of interest in sb is" + sb.rate());
		System.out.println("the rate of interest in ic is" + ic.rate());
		System.out.println("the rate of interest in sb is" + rb.rate());
		rb.loan();
	}

}
OUTPUT :
Exception in thread "main" java.lang.Error: Unresolved compilation problem: 
	The method loan() is undefined for the type RBIbank

	at demoproject/Com.sample.Inheritance.main(Inheritance.java:33)

2.package Com.sample;
class RBIbank{

	public double rate() {
		return 2;
	}
}
class SBI extends RBIbank{
	
	public double rate() {
		return 3;
	}
	public void loan() {
		System.out.println("SBI gives loan up to 1lakh");
	}
}
class ICICI extends RBIbank{
	public double rate() {
		return 4;
	}
}

public class Inheritance {
	public static void main(String[] args) {
		RBIbank b = new RBIbank();
		SBI sb = new SBI();
		ICICI ic = new ICICI();
		RBIbank rb = new SBI();
		System.out.println("the rate of interest in RBI is" + b.rate());
		System.out.println("the rate of interest in sb is" + sb.rate());
		System.out.println("the rate of interest in ic is" + ic.rate());
		System.out.println("the rate of interest in sb is" + rb.rate());
		sb.loan();
	}

}
OUTPUT :
the rate of interest in RBI is2.0
the rate of interest in sb is3.0
the rate of interest in ic is4.0
the rate of interest in sb is3.0
SBI gives loan up to 1lakh

